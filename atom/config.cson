"*":
  autosave:
    enabled: true
  core:
    disabledPackages: [
      "linter-jscs"
      "linter-js-standard"
    ]
    themes: [
      "atom-dark-ui"
      "atom-dark-syntax"
    ]
  editor:
    invisibles:
      cr: false
      eol: false
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
  tabs:
    usePreviewTabs: true
  "tree-view":
    hideVcsIgnoredFiles: false
  welcome:
    showOnStartup: false
